    "staging-pm2": "NODE_ENV=development pm2 startOrRestart ./ecosystem.staging.json",
    "production-pm2": "NODE_ENV=production pm2 startOrRestart ./ecosystem.json --env production",
    "lint": "node_modules/.bin/eslint ./src",
    "lint:fix": "node_modules/.bin/eslint --fix ./src",
